
public class Colchao implements
  colchao_comparable <colchao>{
     private int qtMolas;
     private String modelo;

public Colchao ( int qtMolas,
    String modelo){

    super( );
   this.qtMolas = qtMolas;
    this.modelo = modelo;

}
public int setQtMolas( ){
   return qtMolas;

}

public void setQtMolas(int qtMolas){
     this.qtMolas = qtMolas;

}

public String getModelo ( ) {
return modelo;
}
public void setModelo (String
modelo ){
this.modelo= modelo;
}
 
public int compareTo(colchao
matt)
 return this getModelo()
.compareTo(matt.getModelo());

   }


